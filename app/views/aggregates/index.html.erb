
<div class="container">

      <div class="page-header" id="banner">
        <div class="row">
          <div class="col-lg-8 col-md-7 col-sm-6">
            <h3>Here's all your files</h3>
            <table>
              <thead>
                <tr>
                  <th>File name</th>
                  <th>File type</th>
                  <th colspan="3"></th>
                </tr>
              </thead>

              <tbody>
                <% @aggregates.each do |aggregate| %>
                  <tr>
                    <td><%= aggregate.file_name %></td>
                    <td><%= aggregate.file_type %></td>
                    <td><%= link_to 'Show', aggregate %></td>
                    <td><%= link_to 'Edit', edit_aggregate_path(aggregate) %></td>
                    <td><%= link_to 'Destroy', aggregate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
            <%= link_to 'New Aggregate', new_aggregate_path %>
          </div>
        </div>
    </div>
</div>
